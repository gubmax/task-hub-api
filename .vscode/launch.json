{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Launch debug",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "debug"
      ],
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**",
        "!${workspaceRoot}/node_modules/**/*.js"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}